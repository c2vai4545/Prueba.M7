# Variables de conexión a la base de datos

# Nombre del host de la base de datos
DB_HOST=localhost

# Puerto de la base de datos
DB_PORT=5432

# Nombre de la base de datos
DB_NAME=mi_base_de_datos

# Usuario de la base de datos
DB_USER=usuario

# Contraseña del usuario de la base de datos
DB_PASSWORD=contraseña_segura

# Tipo de base de datos (por ejemplo, postgresql, mysql, etc.)
DB_TYPE=postgresql

# Cadena de conexión completa (opcional, si se prefiere usar en lugar de las variables individuales)
DATABASE_URL=postgresql://usuario:contraseña_segura@localhost:5432/mi_base_de_datos
